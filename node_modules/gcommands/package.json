{
  "name": "gcommands",
  "version": "8.0.3",
  "description": "Powerful and flexible command handler that can do everything!",
  "main": "src/index.js",
  "types": "./typings/index.d.ts",
  "dependencies": {
    "@gcommands/events": "^2.0.6",
